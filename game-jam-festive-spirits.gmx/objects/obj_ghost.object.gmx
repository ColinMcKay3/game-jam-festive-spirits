            <string>//Movement
if(keyboard_check(ord('A'))){
    hspeed = -player_speed;
}else if(keyboard_check(ord('D'))){
    hspeed = player_speed;
}else {
    hspeed = 0;
}

if(keyboard_check(ord('W'))){
    
    vspeed = -player_speed;
}else if(keyboard_check(ord('S'))){
    vspeed = player_speed
}else {
    vspeed = 0;
}

speed = min(speed, player_speed);

//Collision
if(keyboard_check(ord('A'))){
    x_direction = -1;
}else if(keyboard_check(ord('D'))) {
    x_direction = 1;
} else{
    x_direction =0;
}

if(keyboard_check(ord('W'))){
    y_direction = -1;
}else if(keyboard_check(ord('S'))) {
    y_direction = 1;
} else {
    y_direction = 0;
}

if(place_meeting(x+(x_direction*speed),y,obj_wall)){
    hspeed = 0;
    while(!place_meeting(x+x_direction,y,obj_wall)){
        x+=x_direction;
    }
}

if(place_meeting(x,y+(y_direction*speed),obj_wall)){
    vspeed = 0;
    while(!place_meeting(x,y+y_direction,obj_wall)){
        y+=y_direction;
    }
}

//Festive
green_scale = (festive/100)*5;

//the amount of festive you get is kept within obj_present

//Exit The Game
if (keyboard_check_pressed(vk_escape)) {
    room_goto(rm_menu);
}


</string>
